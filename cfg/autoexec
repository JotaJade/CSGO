		bind "f12" "demo_timescale 6"
		//bind "f12" "host_timescale 6"



////////////////////////// 5. Scripts //////////////////////////


	// Netgraph on Scoreboard

		alias 	"+scorenet" 	"+showscores; net_graph 1"
		alias 	"-scorenet" 	"-showscores; net_graph 0"
		bind 	"TAB" 			"+scorenet"


	// Toggle Info Target ID 2017-03-16

		alias 	"ToggleInfo" 	"info_on"
		alias 	"info_on" 		"+cl_show_team_equipment; alias ToggleInfo info_off"
		alias 	"info_off" 		"-cl_show_team_equipment; alias ToggleInfo info_on"
		bind 	"," 			"ToggleInfo"


	// Aligning smokes (You have to match "cl_crosshairsize" to your original crosshair size)

		bind 	"9" 				"smokethrowtoggle"
		alias 	"smokethrowtoggle" 	"smokethrowon"
		alias 	"smokethrowon" 		"cl_crosshairsize 9999; alias smokethrowtoggle smokethrowoff"
		alias 	"smokethrowoff" 	"cl_crosshairsize 2.2; alias smokethrowtoggle smokethrowon"


	// Jumpthrow

		alias 	"+jumpthrow" 	"+attack;+jump;-attack" 
		alias 	"-jumpthrow" 	"-jump" 
		bind 	"8" 			"+jumpthrow"	


	// Clutch Mode
		
		bind 	"p" 			"clutchtoggle"
		alias 	"clutchtoggle" 	"clutchon"
		alias 	"clutchon" 		"voice_enable 0; ignoremsg; ignoremsg; say_team Clutch mode: ON (text & voice chat are disabled); alias clutchtoggle clutchoff"
		alias 	"clutchoff" 	"voice_enable 1; ignoremsg; say_team Clutch mode: OFF (text & voice chat are enabled); alias clutchtoggle clutchon"

		
	//GAME INSTRUCTOR

		bind 	"o" 		"gitoggle"
		alias 	"gitoggle" 	"gion"
		alias 	"gion" 		"gameinstructor_enable 1; echo Game Instructor ON; say_team Game Instructor ON; alias gitoggle gioff"
		alias 	"gioff" 	"gameinstructor_enable 0; echo Game Instructor OFF; say_team Game Instructor OFF; alias gitoggle gion"


	//AFK Script
		
		alias 	startafk 	"echo AFK script ON; +forward; +moveleft; +left;"
		alias 	stopafk 	"echo AFK script OFF; -forward; -moveleft; -left;"


	// Advanced Bindings (by BananaGaming)

		//Part 1 
		//Make sure that what is inside the quotes is what you're currently using.
		//Note: This version lets you create your own values from scratch, KEY is your button, currentvalue is the command.
			alias defA "bind q +voicerecord"
			alias defB "bind shift +speed"
			alias defC "bind 3 slot3"
			alias defD "bind 1 slot1"
			alias defE "bind r +reload"
			alias defF "bind g drop"
			alias defG "bind ctrl +duck"

		//Part 2
		//If you changed any letters/numbers in part 1 make sure you make the same changes here.
		//Don't change the keyXX part.
			alias keyA "bind q keyAA"
			alias keyB "bind shift keyBB" 
			alias keyC "bind 3 keyCC"
			alias keyD "bind 1 keyDD"
			alias keyE "bind r keyEE"
			alias keyF "bind g keyFF"
			alias keyG "bind ctrl keyGG"

		//Part 3
		//Here you can change what the bindings do when you hold down your key.
		//This is the fun part. :)
			alias keyAA "lastinv"
			alias keyBB "noclip"
			alias keyCC "slot5"
			alias keyDD "volume 1"
			alias keyEE "r_cleardecals"
			alias keyFF "use weapon_c4;drop"
			alias keyGG "ToggleInfo"

		//Part 4 - Don't touch!
			alias +keybinds "keyA; keyB; keyC; keyD; keyE; keyF; keyG"
			alias -keybinds "defA; defB; defC; defD; defE; defF; defG"

		//Part 5 - Bind whatever key you want, this will be your main key
			bind alt +keybinds



