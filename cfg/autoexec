// Thank you to BananaGaming and PinkFreud for helping me build this config file.

//---Content

	// 1. General Settings
	// 2. Game Settings
	// 3. Visual Settings
	// 4. Bindings
	// 5. Scripts
	// 6. Aliases



////////////////////////// 1. General Settings //////////////////////////


	// Network

		sv_maxrate 							"0" 
		sv_minrate 							"30000"
		sv_mincmdrate 						"128"
		sv_minupdaterate 					"128"
		sv_maxupdaterate					"128"
		rate 								"128000" 		//Max bytes per second the host can receive data
		cl_cmdrate 							"128" 			//Max number of command packets sent to server per second
		cl_updaterate 						"128" 			//Number of packets per second you are requesting from the server
		cl_interp_ratio 					"2" 			//Sets the interpolation amount (final amount is cl_interp_ratio / cl_updaterate)
		cl_interp 							"0"				//Sets the interpolation amount
		cl_lagcompensation 					"1"				//Lag compensation helps by eliminating combat latency from client side view
		cl_predict 							"1"				//Skip waiting for server feedback and simulate client side movement in real-time
		cl_predictweapons 					"1" 			//Skip waiting for server feedback and perform client side prediction of weapon effects
		mm_dedicated_search_maxping 		"80"			//Max ping allowed when searching for a game server


	// Menus & Interface

		con_enable 							"1"				//Enables or disables console	
		fps_max 							"0"				//Sets the FPS cap ("0" for no cap)
		fps_max_menu 						"9999"			//Sets the FPS cap for the menu
		cl_showfps 							"0"				//Shows FPS
		net_graph 							"0"				//Enables or disables net_graph
		net_graphpos 						"2"				//Sets the net_graph position
		cl_showpos 							"0"				//Shows position related values
		cl_color 							"3"				//Sets the minimap player dot color ("0" = yellow, "1" = purple, "2" = green, "3" = blue, "4" = orange)
		snd_mixahead 						"0.05"			//Makes sound as instant as it can get, making it easier to hear small things
		cl_showhelp 						"0" 			//Removes on-screen help
		cl_autohelp 						"0" 			//Removes automatic help
		cl_disablehtmlmotd 					"1"  			//Disables community server ads


	// Audio

		voice_enable						"1"				//Enables or disables voice chat
		snd_mute_losefocus 					"0"				//Game mutes if minimized ("0" to disable it)
		lobby_voice_chat_enabled 			"0"				//Turns microphone off in lobby, really useful



////////////////////////// 2. Game Settings //////////////////////////


	// Game

		r_drawtracers_firstperson 			"1"
		cl_hud_background_alpha 			"1"
		cl_autowepswitch 					"0"
		cl_use_opens_buy_menu 				"0"

	// Mouse

		m_rawinput 							"1"
		sensitivity         				"1"
		zoom_sensitivity_ratio_mouse 		"1"
		m_mouseaccel2 						"0"
		m_mouseaccel1 						"0"
		m_customaccel 						"0"



////////////////////////// 3. Visual Settings //////////////////////////


	// Hud

		safezonex                           "1"				//Horizontal hud offset
		safezoney                           "1"				//Vertical hud offset
		hud_scaling                         "0.85"			//Hud scale
		+cl_show_team_equipment								//Shows teammates' equipment
		cl_teamid_overhead_mode 			"2"				//Always show team behind walls with equipment
		cl_teamid_overhead_always 			"1"				//Shows teammates' equipment overhead
		cl_hud_bomb_under_radar             "1"				//Bomb icon shows under the radar if you're carrying it
		hud_showtargetid 					"1" 			//Enables display of target names
		cl_hud_playercount_showcount 		"1"				//Shows player avatars instead of numbers left
		cl_showloadout 						"0"				//Always shows loadout ("0" to fade it out)


	// Radar

		cl_radar_scale      				"0.4" 			//Radar zoom scale
		cl_hud_radar_scale 					"1.2"			//Radar size
		cl_radar_icon_scale_min 			"0.4" 			//Player size in radar (min="0.4", default="0.6", max="1")
		cl_radar_always_centered  			"0"				//Centers radar ("0" to disable it)
		cl_teammate_colors_show 			"0"				//Show teammates as separate colors without letter ("0" for letter)


	// Viewmodel

		viewmodel_fov 						"68"
		viewmodel_offset_z 					"-2"
		viewmodel_offset_y 					"2"
		viewmodel_offset_x 					"1"


	// Crosshair

		cl_crosshair_drawoutline 					"1"
		cl_crosshair_dynamic_maxdist_splitratio 	"0"
		cl_crosshair_dynamic_splitalpha_innermod 	"1"
		cl_crosshair_dynamic_splitalpha_outermod 	"0.300000"
		cl_crosshair_dynamic_splitdist 				"5"
		cl_crosshair_outlinethickness 				"1"
		cl_crosshair_sniper_width 					"2"
		cl_crosshairalpha 							"255"
		cl_crosshaircolor 							"5"
		cl_crosshaircolor_b 						"255"
		cl_crosshaircolor_g 						"255"
		cl_crosshaircolor_r 						"255"
		cl_crosshairdot 							"0"
		cl_crosshairgap 							"-1"
		cl_crosshairgap_useweaponvalue 				"0"
		cl_crosshairscale 							"0"
		cl_crosshairsize 							"2.2"
		cl_crosshairstyle 							"4"
		cl_crosshairthickness 						"0.6"
		cl_crosshairusealpha 						"1"
		cl_fixedcrosshairgap 						"-10"



